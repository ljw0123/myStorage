package string;

public class Main04 {

	public static void main(String[] args) {
		// 기본 자료형을 문자열로 변환하는 방법
		// wrapper : String -> 기본자료형
		// 기본자료형 -> String
		
		int a = 1;
		float b = 3.14f;
		boolean c = true;
		double d = 123.4567d;
		long e = 12345678912346L;
		
		/*
		 * 
		 */
		
		
		
		
	}

}













