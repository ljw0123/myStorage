package thread;

public class ThreadInterruptTest extends Thread {

	@Override
	public void run() {
		try {
			
			
		} catch (InterruptedException e) {
			e.printStackTrace();
			System.out.println("스레드 종료");
			// 스레드 함수 내에서 return 문을 통해서 
			// 종료 및 소멸시키는 방법이 가장 이상적
			return;
		}
		
	}
	
	
}




