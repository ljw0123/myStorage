package file;

import java.io.File;

public class Main01 {

	public static void main(String[] args) {
		// 이클립스에서 상대경로를 사용할 경우, 
		// 현재 프로젝트가 기준이 된다. 
		// 절대경로나 or 상대경로를 생성자 파라미터로 전달
		File f1 = new File("src/file/Main01.java");
		
		// 전달된 경로가 파일인지 검사
		// -> 존재하지 않는 파일로 검사할 경우 무조건 false
		boolean is_file = f1.isFile();
		
		
	}

}












